  issn            = {1091-6490},
  issn-linking    = {0027-8424},
  issue           = {14},
  journal         = {Proceedings of the National Academy of Sciences of the United States of America},
  keywords        = {Brain, physiology; Humans; Magnetic Resonance Imaging; Models, Theoretical; Task Performance and Analysis},
  month           = apr,
  nlm-id          = {7505876},
  owner           = {NLM},
  pages           = {5487--5492},
  pii             = {1121049109},
  pmc             = {PMC3325687},
  pmid            = {22431587},
  pubmodel        = {Print-Electronic},
  revised         = {2021-10-21},
  title           = {Whole-brain, time-locked activation with simple tasks revealed using massive averaging and model-free analysis.},
