  issue           = {5},
  journal         = {{IEEE} Transactions on Medical Imaging},
  keywords        = {Algorithms; Brain, blood supply, physiology; Computer Simulation; Humans; Magnetic Resonance Imaging, methods; Signal Processing, Computer-Assisted},
  month           = {may},
  nlm-id          = {8310780},
  number          = {5},
  owner           = {NLM},
