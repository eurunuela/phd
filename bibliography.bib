  month     = {may},
  number    = {1},
  pages     = {200--207},
  publisher = {Elsevier {BV}},
  title     = {Modeling regional and psychophysiologic interactions in {fMRI}: the importance of hemodynamic deconvolution},
  volume    = {19},
  year      = {2003}
}

